{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "Canada East"
    },
    "baseUrl": {
      "value": "https://gallery.azure.com/artifact/20151001/bitnami.production-mongodbdefault.1.0.1/Artifacts"
    },
    "stackId": {
      "metadata": {
        "description": "Stack ID - it is the Application Stack identifier."
      },
        "value": "bit"
    },
    "sshKey": {
      "metadata": {
        "description": "SSH rsa public key file as a string."
      },
      "value": "ssh-rsa  ops-master@nurun.com"
    },
    "vmSize": {
      "metadata": {
        "description": "Size of the VM"
      },
      "value": "Standard_DS1"
    },
    "numReplicas": {
      "value": 2
    },
    "arbiterCount": {
      "value": 1
    }
  }
}
